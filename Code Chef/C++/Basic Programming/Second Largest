#include <iostream>
using namespace std;

int main() {
	int t,a,b,c;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    cin>>a>>b>>c;
	    if(a>b && a>c){
	        if(b<c)cout<<c<<endl;
	        else if (c<b)cout<<b<<endl;
	    }
	    else if (b>a && b>c){
	        if(a<c)cout<<c<<endl;
	        else if (a>c)cout<<a<<endl;
	    }
	    else if (c>a && c>b){
	        if(a<b)cout<<b<<endl;
	        else if (a>b)cout<<a<<endl;
	    }
	}
	return 0;
}
