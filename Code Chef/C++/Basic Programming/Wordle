#include <iostream>
using namespace std;

int main() {
	int t;
	char s1[5],s2[5],m[5];
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    for(int j=0;j<5;j++)
	    {
	     cin>>s1[j];   
	    }
	    for(int j=0;j<5;j++)
	    {
	     cin>>s2[j];   
	    }
	    for(int j=0;j<5;j++)
	    {
	        if(s1[j]==s2[j]) m[j]='G';
	        else m[j]='B';
	    }
	    for(int j=0;j<5;j++)
	    {
	        cout<<m[j];
	    }
	    cout<<endl;
	    
	}
	return 0;
}
