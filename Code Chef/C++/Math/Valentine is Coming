#include <iostream>
using namespace std;

int main() {
	int t,x,y,count=0;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    cin>>x>>y;
	    if(y>x)cout<<"0"<<endl;
	    else
	    {
	        while(true)
	        {
	            if(x<y)break;
	            x-=y;
	            count++;
	        }
	        cout<<count<<endl;
	        count=0;
	    }
	}
	return 0;
}
