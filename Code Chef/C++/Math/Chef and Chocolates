#include <iostream>
using namespace std;

int main() {
	int t,total=0,count=0,x,y,z;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    cin>>x>>y>>z;
	    total=(5*x)+(10*y);
	    while(total>=z)
	    {
	        count++;
	        total-=z;
	        
	    }
	    
	    cout<<count<<endl;
	    total=0;
	    count=0;
	}
	return 0;
}
